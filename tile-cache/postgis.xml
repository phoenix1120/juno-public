<?xml version="1.0" encoding="utf-8"?>
<Map srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs" background-color="rgb(70,130,180)">
    <Style name="My Style">
        <Rule>
            <PolygonSymbolizer fill="rgb(0,128,0)"/>
            <LineSymbolizer stroke="rgb(255,140,0)" stroke-width="2"/>
        </Rule>
    </Style>
    <Style name="My Style2">
        <Rule>
            <PolygonSymbolizer fill="rgb(70,130,180)"/>
            <LineSymbolizer stroke="rgb(0,0,139)" stroke-width="3"/>
        </Rule>
    </Style>
    <Layer name="shape" srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs">
        <StyleName>My Style</StyleName>
        <Datasource>
            <Parameter name="dbname">falcon</Parameter>
            <Parameter name="host">localhost</Parameter>
            <Parameter name="password">shotfirst</Parameter>
            <Parameter name="table">world</Parameter>
            <Parameter name="type">postgis</Parameter>
            <Parameter name="user">postgres</Parameter>
        </Datasource>
    </Layer>
</Map>
